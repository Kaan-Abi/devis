<form method="post" action="{{ path('app_devis_delete', {'reference': devi.reference}) }}" onsubmit="return confirm({{ "form.confirm"|trans({},'interface') }});">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ devi.reference) }}">
    <button class="btn btn-danger">
        {{ "delete"|trans({}, 'interface') }}
    </button>
</form>

